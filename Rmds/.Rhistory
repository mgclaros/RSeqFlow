mpg
mpg_summarized_tbl <- mpg %>%
select(-year) %>%
group_by(class) %>%
summarise(
across(
.cols  = where(is.numeric),
.fns   = median,
.names = "{.col}_median"
),
count = n()
) %>%
ungroup() %>%
mutate(
prop       = count / sum(count),
all_groups = "all_groups",
class      = fct_reorder(class, prop)
)
