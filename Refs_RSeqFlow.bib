	mesh = {Artifacts; Gene Regulatory Networks; Genetic Techniques; Humans},
	month = {05},
	number = {1},
	pages = {94},
	pmc = {PMC6521369},
	pmid = {31097038},
	pst = {epublish},
	title = {Addressing confounding artifacts in reconstruction of gene co-expression networks},
