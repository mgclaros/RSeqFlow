	mesh = {Bayes Theorem; Computational Biology; Computer Simulation; Databases, Nucleic Acid; Gene Expression Profiling; Humans; Markov Chains; Models, Statistical; Molecular Sequence Annotation; Sequence Analysis, RNA; Signal-To-Noise Ratio; Software; Time Factors},
	month = {01},
